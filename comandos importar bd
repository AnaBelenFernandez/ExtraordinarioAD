mongoimport --authenticationDatabase admin --db formacion --collection alumnos --file alumnos.json --jsonArray -u root

mongoimport --authenticationDatabase admin -u root --db formacion --collection alumnos --out alumnos_completo.json


docker cp alumnos.json mongo-server:/root
 internet
sudo mongoimport --db formacion --collection alumnos --file alumnos.json
mongoimport --host mongo-server --db formacion --collection alumnos --type json --file alumnos.json --jsonArray 
